<odoo>
  <data>
    <record model="ir.ui.view" id="filesharing_view_file_tree_view">
      <field name="name">File Sharing File View</field>
      <field name="model">filesharing.file</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name" />
          <field name="link" />
          <field name="type" />
          <field name="parent" />
          <field name="tags" />
          <field name="related_project" />
        </tree>
      </field>
    </record>
  </data>
</odoo>
